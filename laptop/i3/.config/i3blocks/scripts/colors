#!/bin/bash
# vim: ft=sh
all_colors=$(cat ~/.Xresources | grep '^\*\.color[0-9]*:\ ')
fullblack='#000000'
black=$(echo "$all_colors" | grep '\.color0' | grep -o '\#[a-zA-Z0-9]*')
brightblack=$(echo "$all_colors" | grep '\.color8' | grep -o '\#[a-zA-Z]*[0-9]*')
red=$(echo "$all_colors" | grep '\.color1' | grep -o '\#[a-zA-Z0-9]*')
brightred=$(echo "$all_colors" | grep '\.color9' | grep -o '\#[a-zA-Z0-9]*')
green=$(echo "$all_colors" | grep '\.color2' | grep -o '\#[a-zA-Z0-9]*')
brightgreen=$(echo "$all_colors" | grep '\.color10' | grep -o '\#[a-zA-Z0-9]*')
yellow=$(echo "$all_colors" | grep '\.color3' | grep -o '\#[a-zA-Z0-9]*')
brightyellow=$(echo "$all_colors" | grep '\.color11' | grep -o '\#[a-zA-Z0-9]*')
blue=$(echo "$all_colors" | grep '\.color4' | grep -o '\#[a-zA-Z0-9]*')
brightblue=$(echo "$all_colors" | grep '\.color12' | grep -o '\#[a-zA-Z0-9]*')
magenta=$(echo "$all_colors" | grep '\.color5' | grep -o '\#[a-zA-Z0-9]*')
brightmagenta=$(echo "$all_colors" | grep '\.color13' | grep -o '\#[a-zA-Z0-9]*')
cyan=$(echo "$all_colors" | grep '\.color6' | grep -o '\#[a-zA-Z0-9]*')
brightcyan=$(echo "$all_colors" | grep '\.color14' | grep -o '\#[a-zA-Z0-9]*')
white=$(echo "$all_colors" | grep '\.color7' | grep -o '\#[a-zA-Z0-9]*')
brightwhite=$(echo "$all_colors" | grep '\.color15' | grep -o '\#[a-zA-Z0-9]*')
