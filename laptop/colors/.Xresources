! vim: ft=xdefaults commentstring=!%s
! -----  define colors  ----- !
#define nord0  #2E3440  /* almost black */
#define nord1  #3B4252  /* dark gray  */
#define nord2  #434C5E  /* lighter gray */
#define nord3  #4C566A  /* dark gray-blue */
#define nord4  #D8DEE9  /* darkest-white */
#define nord5  #E5E9F0  /* dark-white */
#define nord6  #ECEFF4  /* light-white*/
#define nord7  #8FBCBB  /* turquoise */
#define nord8  #88C0D0  /* cyan */
#define nord9  #81A1C1  /* light gray-blue*/
#define nord10 #5E81AC  /* dark-blue */
#define nord11 #BF616A  /* red */
#define nord12 #D08770  /* orange */
#define nord13 #EBCB8B  /* yellow */
#define nord14 #A3BE8C  /* green */
