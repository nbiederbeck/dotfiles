# vim: commentstring=#%s
super + Return
    alacritty

super + p
    ~/.local/dotbin/pubslist

super + shift + p
    ~/.local/dotbin/bwmenu

super + d
    i3-dmenu-desktop

XF86AudioRaiseVolume
    ~/.local/dotbin/sound.sh increase speakers
XF86AudioLowerVolume
    ~/.local/dotbin/sound.sh decrease speakers
XF86AudioMute
    ~/.local/dotbin/sound.sh toggle-mute speakers
XF86AudioMicMute
    ~/.local/dotbin/sound.sh toggle-mute microphone

XF86MonBrightnessUp
    ~/.local/dotbin/brightness increase
XF86MonBrightnessDown
    ~/.local/dotbin/brightness decrease
super + XF86MonBrightnessUp
    ~/.local/dotbin/brightness set 100
super + XF86MonBrightnessDown
    ~/.local/dotbin/brightness set 1

# F7
XF86Display
    ~/.local/dotbin/i3-randr
super + XF86Display
    arandr
# F8
# XF86WLAN (mapped)
# F9
# XF86Tools
# F10
# XF86Search

super + Print
    gnome-screenshot

# super + shift + d
#     rofi -show window -lines 5
