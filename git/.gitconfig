; vim: ft=gitconfig
[core]
	editor = nvim
[user]
	name = Noah Biederbeck
	email = noah.biederbeck@udo.edu
[pull]
	rebase = true
[merge]
	conflictstyle = diff3
[credential]
	helper = cache --timeout=10800
[alias]
    l = log --graph
    ll = log --color --graph --date=short --pretty=format:\"%Cblue%ad %Cgreen%an %C(yellow)%h %Creset%s %Cred%d %n%Creset%b\"
    co = commit -m
    s = status
